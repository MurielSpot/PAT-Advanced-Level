/*
https://www.patest.cn/contests/pat-a-practise/1035
类型：字符串处理

难度：1

注意事项：
1、当没有账户被更改时，如果账户多余1个，英语语句的语法要随之改变，用对应的复数are和加s。
*/ 

#include<cstdio>
#include<cstring>
#include<algorithm>
#include<vector>
using namespace std;

const int MAXN=15;
struct info{
	char name[MAXN];
	char pa[MAXN];
};

vector<struct info> change;

bool modify(struct info *t){
	int lenPa=strlen(t->pa);
	bool flag=false;
	for(int i=0;i<lenPa;i++){
		if(t->pa[i]=='1'){
			t->pa[i]='@';flag=true;
		}else if(t->pa[i]=='0'){
			t->pa[i]='%';flag=true;
		}else if(t->pa[i]=='l'){
			t->pa[i]='L';flag=true;
		}else if(t->pa[i]=='O'){
			t->pa[i]='o';flag=true;
		}
	}
	return flag;
}

int main(){
	int n;
	scanf("%d",&n);
	for(int i=0;i<n;i++){
		info t;
		scanf("%s%s",t.name,t.pa);
		if(modify(&t)){
			change.push_back(t);
		}
	}
	if(change.size()==0){
		if(n==1){//要区分录入账户是单数还是复数！
			printf("There is 1 account and no account is modified");
		}else{
			printf("There are %d accounts and no account is modified",n);
		}
	}else{
		printf("%d\n",change.size());
		for(int i=0;i<change.size();i++){
			printf("%s %s\n",change[i].name,change[i].pa);
		}
	}

	return 0;
}
